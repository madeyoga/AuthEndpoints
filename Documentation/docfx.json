{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/AuthEndpoints.Core"
        },
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/AuthEndpoints.SimpleJwt"
        },
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/AuthEndpoints.SimpleJwt.Core"
        },
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/AuthEndpoints.TokenAuth"
        },
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/AuthEndpoints.TokenAuth.Core"
        },
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/AuthEndpoints.MinimalApi"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "wiki/**.md",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "AuthEndpoints",
      "_description": "An authentication library for ASP.Net 6",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_appFaviconPath": "images/favicon.ico",
      "_appFooter": "Developed by <a href='https://github.com/madeyoga/AuthEndpoints'>MadeY</a> and <a href='https://github.com/madeyoga/AuthEndpoints/graphs/contributors'>contributors</a> / Licensed under <a href='#'>MIT</a> / Website generated by <a href='https://dotnet.github.io/docfx/'>DocFX</a>",
      "_gitContribute": {
        "branch": "main"
      },
      "_gitUrlPattern": "github"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}